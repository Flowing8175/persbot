# Discord Bot Token
DISCORD_TOKEN=your_discord_token_here

# LLM Provider Settings
# Options: gemini, openai, zai
ASSISTANT_LLM_PROVIDER=gemini
SUMMARIZER_LLM_PROVIDER=gemini

# Model Selection (Role-Specific Overrides)
# Override default models by provider and role:
# GEMINI_ASSISTANT_MODEL_NAME, GEMINI_SUMMARY_MODEL_NAME
# OPENAI_ASSISTANT_MODEL_NAME, OPENAI_SUMMARY_MODEL_NAME
# ZAI_ASSISTANT_MODEL_NAME, ZAI_SUMMARY_MODEL_NAME
# Example: GEMINI_ASSISTANT_MODEL_NAME=gemini-2.5-pro

# API Keys (Required based on provider)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
ZAI_API_KEY=your_zai_api_key_here

# Z.AI API Base URL (optional)
# Standard API: https://api.z.ai/api/paas/v4/ (pay-as-you-go, token-based)
# Coding Plan API: https://api.z.ai/api/coding/paas/v4/ (subscription-based, prompt-based)
# Set ZAI_CODING_PLAN=true to use Coding Plan API endpoint automatically
ZAI_BASE_URL=https://api.z.ai/api/paas/v4/
# Enable Z.AI Coding Plan API (uses subscription quota instead of pay-as-you-go)
# Options: true, false (default: false)
ZAI_CODING_PLAN=false

# Auto Reply Channels (Comma separated IDs)
AUTO_REPLY_CHANNEL_IDS=

# Application Settings
LOG_LEVEL=INFO
SERVICE_TIER=flex

# Message Buffer Settings (Seconds)
MESSAGE_BUFFER_DELAY=2.5
# Max Chat Hisorty Turns (20 recommended)
MAX_HISTORY=20

# LLM Temperature (0.0 to 2.0)
TEMPERATURE=1.0
# LLM TOP_P (0.0 to 1.0)
TOP_P=0.9

# Bypass Discord Permission Checks
NO_CHECK_PERMISSION=false
